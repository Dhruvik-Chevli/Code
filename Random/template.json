{ 
    "Template": {
        "prefix": "init",
        "body": [
        "#include <iostream>",
        "#include<vector>",
        "#include<algorithm>",
        "#include<utility>",
        "#include<string>",
        "#include<unordered_set>",
        "#include<stack>",
        "#include<queue>",
        "#include<unordered_map>",
        "using namespace std;",
        "typedef long long int ll;",
        "#define EPS 1e-9",
        "#define pb push_back",
        "#define FOR(i, a, b) for(ll i = a; i < b; i++)",
        "#define PI 3.1415926535897932384626433832795",
        "#define MOD 1000000007",
        "bool isPrime(int n) ",
        "{" ,
        "    if (n <= 1)  return false; ",
        "    if (n <= 3)  return true;" ,
        "    if (n%2 == 0 || n%3 == 0) return false;",
        "    for (int i=5; i*i<=n; i=i+6) ",
        "        if (n%i == 0 || n%(i+2) == 0) ",
        "          return false;",
        "    return true; ",
        "}",
        "ll power(ll x,ll y,ll p)",
        "{",
        "    ll ans=1;",
        "    x = x%p;",
        "    if(x==0) return 0;",
        "    while(y)",
        "    {",
        "        if(y&1) ans=(ans*x)%p;",
        "        y>>=1;",
        "        x=(x*x)%p;",
        "    }",
        "    return ans;",
        "}",
        "int main()",
        "{",
        "	std::ios_base::sync_with_stdio(false);",
        "	cin.tie(NULL);",
        "	return 0;",
        "}"
        ],
        "description": "Basic cpp template"
    }
}